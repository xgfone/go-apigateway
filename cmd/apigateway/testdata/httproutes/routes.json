[
    {
        "id": "route1",
        "upstream": "upstream",
        "matchers": [
            {
                "methods": [
                    "get"
                ],
                "paths": [
                    "/path1"
                ]
            }
        ],
        "middlewares": [
            {
                "name": "allow",
                "conf": [
                    "127.0.0.0/8"
                ]
            }
        ]
    },
    {
        "id": "route2",
        "upstream": "upstream",
        "matchers": [
            {
                "methods": [
                    "get"
                ],
                "paths": [
                    "/path2"
                ]
            }
        ],
        "middlewareGroup": "onlydeny127"
    },
    {
        "id": "route3",
        "protect": true, // never match the request.
        "upstream": "upstream",
        "matchers": [
            {
                "methods": [
                    "get"
                ],
                "paths": [
                    "/path3"
                ]
            }
        ]
    }
]
