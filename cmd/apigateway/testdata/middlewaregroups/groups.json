{
    // Only allow 127.0.0.0/8 and deny others.
    "onlyallow127": {
        "allow": {
            "cidrs": [
                "127.0.0.0/8"
            ]
        }
    },
    // Only deny 127.0.0.0/8 and allow others.
    "onlydeny127": {
        "block": {
            "cidrs": [
                "127.0.0.0/8"
            ]
        }
    }
}
